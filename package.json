{
  "name": "bfast-cloud",
  "description": "bfast cloud orchestration for project management",
  "version": "1.0.0-alpha.2",
  "private": false,
  "main": "lib/bfast-cloud.js",
  "scripts": {
    "start": "node ./lib/index.js",
    "start:dev": "gulp devStart",
    "build": "gulp",
    "build:docker": "gulp buildDocker",
    "poststart:dev": "ts-node-dev --respawn --transpileOnly script ./src/index.ts",
    "test": "npm run build && mocha --timeout 60000000 ./specs",
    "push:docker": "gulp pubishDockerImage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "morgan": "^1.10.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/redis-mock": "^0.17.0",
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "mongodb-memory-server-core": "^6.9.2",
    "redis-mock": "^0.55.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
